    "# First success\n",
    "\n",
    "d = [2,2,3,3]\n",
    "budget = 10\n",
    "\n",
    "def solution(d, budget):\n",
    "    result = []\n",
    "    sorted_d = sorted(d)\n",
    "\n",
    "    for i in sorted_d:\n",
    "        if i <= budget:\n",
    "            result.append(i)\n",
    "            budget -= i\n",
    "    return len(result)\n",
    "\n",
    "solution(d,budget)"
   ]
